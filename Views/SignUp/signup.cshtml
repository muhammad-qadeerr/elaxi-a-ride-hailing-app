@using Ride_Booking_Application.Models;
@model MyLoginSignup;


<!-- inner page section -->
<section class="inner_page_head">
    <div class="container_fuild">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <h3>Sign Up</h3>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- end inner page section -->
<!-- why section -->
<section class="why_section layout_padding">
    <div class="container px-10 py-2">

        <div class="row">
            <div class="col-lg-8 offset-lg-2">
                <div class="full mb-3 row px-3 py-0"
                    <form action="/Home/home" method="post">
                        <label asp-for="Username" required>Enter a Username: </label>
                        <input asp-for ="Username" type = "text"/>
                        <label asp-for="Name" required >Enter your Name: </label>
                        <input asp-for ="Name" type="text"/>
                        <label asp-for="Password" required >Enter your password: </label>
                        <input asp-for="Password" type="text"/><br />
                        <label asp-for="PhoneNo"required>Enter your phone number: </label><br />
                        <input asp-for="PhoneNo" type = "text"/><br /><br/>
                        <button class="btn btn-success text-white " type="submit">
                            Signup
                        </button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- end arrival section -->
